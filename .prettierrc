{
  "arrowParens": "always",
  "semi": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "endOfLine": "auto",
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 120,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports"],
  "tailwindFunctions": ["clsx"],
  "tailwindAttributes": ["dark", "className", "classNames"]
}
